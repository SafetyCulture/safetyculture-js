{
  "name": "safetyculture",
  "version": "0.0.1",
  "description": "Javascript SDK for SafetyCulture API",
  "main": "lib/safetyculture.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/ test/",
    "test": "npm run lint && BABEL_ENV=TEST mocha --compilers js:babel-core/register",
    "compile": "babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/safetyculture/safetyculture-js.git"
  },
  "keywords": [],
  "author": "Nick Matenaar <nick@prismatik.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safetyculture/safetyculture-js/issues"
  },
  "homepage": "https://github.com/safetyculture/safetyculture-js/",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-rewire": "^1.0.0-beta-3",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.2.0",
    "del": "^1.1.1",
    "eslint": "^1.10.3",
    "glob": "^5.0.14",
    "isparta": "^3.0.3",
    "json-loader": "^0.5.3",
    "mocha": "^2.1.0",
    "rewire": "^2.4.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-stream": "^2.1.1"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "safetyculture",
    "mainVarName": "SafetyCulture",
    "mochaGlobals": [
      "stub",
      "spy",
      "expect",
      "sandbox",
      "mock",
      "useFakeTimers",
      "useFakeXMLHttpRequest",
      "useFakeServer"
    ]
  },
  "dependencies": {
    "babel-template": "^6.3.13",
    "babel-types": "^6.3.14",
    "lodash": "^3.10.1",
    "request-promise": "^1.0.2"
  }
}
