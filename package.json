{
  "name": "safetyculture",
  "version": "0.0.1",
  "description": "Javascript SDK for SafetyCulture API",
  "main": "dist/safetyculture.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "karma start karma.conf.js",
    "check": "npm run lint && npm run test",
    "build": "webpack src/safetyculture.js dist/safetyculture.js --config webpack.config.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/safetyculture/safetyculture-js.git"
  },
  "keywords": [],
  "author": "Nick Matenaar <nick@prismatik.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safetyculture/safetyculture-js/issues"
  },
  "homepage": "https://github.com/safetyculture/safetyculture-js/",
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.2.0",
    "del": "^1.1.1",
    "glob": "^5.0.14",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-expect": "^1.1.1",
    "karma-mocha": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.5.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-stream": "^2.1.1"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "request": "2.65.0",
    "request-promise": "^1.0.2"
  }
}
